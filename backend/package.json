{
  "name": "en_route_backend",
  "version": "0.0.1",
  "description": "The backend for En Route",
  "main": "lib/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "start:dev": "asb build -d src --target debug && node dist/index.js",
    "dev": "tsup src/index.ts --dts --watch src --watch assembly --ignore-watch lib --onSuccess \"npm run start:dev\"",
    "build": "asb build -d src && tsup src/index.ts --dts"
  },
  "keywords": [
    "assemblyscript",
    "express",
    "typescript",
    "node.js"
  ],
  "tsup": {
    "splitting": false,
    "sourcemap": false,
    "clean": false,
    "entryPoints": [
      "src/index.ts"
    ]
  },
  "author": "vicli268 emmse713 jacmo699",
  "license": "ISC",
  "dependencies": {
    "@assemblyscript/loader": "^0.19.20",
    "dotenv": "^8.2.0",
    "fastify": "^3.24.0",
    "fastify-cors": "^6.0.2",
    "fastify-socket.io": "^2.0.1",
    "lodash.clonedeep": "^4.5.0",
    "socket.io": "^4.0.1"
  },
  "devDependencies": {
    "@types/lodash.clonedeep": "^4.5.6",
    "@types/node": "^16.11.7",
    "asbuild": "^0.2.0",
    "assemblyscript": "^0.19.20",
    "concurrently": "^6.0.2",
    "git-format-staged": "^2.1.1",
    "husky": "^6.0.0",
    "prettier": "^2.2.1",
    "tsup": "^5.7.0",
    "typescript": "^4.4.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "git-format-staged -f 'prettier --ignore-unknown --stdin --stdin-filepath \"{}\"' ."
    }
  }
}
